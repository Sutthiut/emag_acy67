import 'package:flutter/material.dart';
import 'package:firebase_database/firebase_database.dart';
import 'package:fl_chart/fl_chart.dart';
import 'dart:math'; // ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏ö‡∏£‡∏£‡∏ó‡∏±‡∏î‡∏ô‡∏µ‡πâ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏™‡∏∏‡πà‡∏° (Random)

String _buildSummary(double percent, int correctTotal,
      int incorrectTotal, int unknownTotal,
      Map<String, int> detailCount) {

    if (correctTotal + incorrectTotal == 0) {
      return "‡∏ß‡∏±‡∏ô‡∏ô‡∏µ‡πâ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡πà‡∏á‡∏ó‡∏µ‡πà‡∏õ‡∏£‡∏∞‡πÄ‡∏°‡∏¥‡∏ô‡πÑ‡∏î‡πâ\n"
          "‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡∏õ‡∏£‡∏±‡∏ö‡πÄ‡∏ó‡∏µ‡∏¢‡∏ö‡∏≠‡∏∏‡∏õ‡∏Å‡∏£‡∏ì‡πå‡∏Å‡πà‡∏≠‡∏ô‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏Ñ‡πà‡∏∞";
    }

    String percentText = percent.toStringAsFixed(1);

    if (incorrectTotal == 0) {
      return "‡∏¢‡∏≠‡∏î‡πÄ‡∏¢‡∏µ‡πà‡∏¢‡∏°‡∏°‡∏≤‡∏Å üëè\n"
          "‡∏ß‡∏±‡∏ô‡∏ô‡∏µ‡πâ‡∏Ñ‡∏∏‡∏ì‡∏ô‡∏±‡πà‡∏á‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á‡∏ï‡∏•‡∏≠‡∏î‡∏ó‡∏±‡πâ‡∏á‡∏ß‡∏±‡∏ô "
          "($percentText%) ‡∏£‡∏±‡∏Å‡∏©‡∏≤‡πÅ‡∏ö‡∏ö‡∏ô‡∏µ‡πâ‡πÑ‡∏ß‡πâ‡∏ô‡∏∞‡∏Ñ‡∏∞ üíö";
    }

    // ‡∏´‡∏≤‡∏õ‡∏±‡∏ç‡∏´‡∏≤‡∏ó‡∏µ‡πà‡∏û‡∏ö‡∏ö‡πà‡∏≠‡∏¢‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î (Main Issue) ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡∏≥‡πÑ‡∏õ‡πÅ‡∏ó‡∏£‡∏Å‡πÉ‡∏ô‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°
    String mainIssue = "‡∏ú‡∏¥‡∏î‡∏ó‡πà‡∏≤";
    if (detailCount.isNotEmpty) {
      var sorted = detailCount.entries.toList()
        ..sort((a, b) => b.value.compareTo(a.value));
      int topCount = sorted.first.value;
      List<String> topProblems = sorted
          .where((e) => e.value == topCount)
          .map((e) => e.key)
          .toList();
      mainIssue = topProblems.join(" + ");
    }

    final random = Random();
    String adviceText = "";

    if (percent >= 80) {
      // --- ‡πÇ‡∏ã‡∏ô‡∏™‡∏µ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ß: ‡∏ä‡∏°‡πÄ‡∏ä‡∏¢‡πÅ‡∏•‡∏∞‡πÉ‡∏´‡πâ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡πÉ‡∏à (50 ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°) ---
      List<String> goodMsgs = [
        "üéâ ‡∏¢‡∏≠‡∏î‡πÄ‡∏¢‡∏µ‡πà‡∏¢‡∏°‡∏°‡∏≤‡∏Å‡∏Ñ‡∏£‡∏±‡∏ö‡∏Ñ‡∏∏‡∏ì! ‡πÑ‡∏î‡πâ‡πÑ‡∏õ $percentText% ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πà‡∏á‡πÄ‡∏õ‡πä‡∏∞‡∏™‡∏∏‡∏î‡πÜ",
        "üåü ‡∏´‡∏∏‡πà‡∏ô‡∏î‡∏µ‡∏ö‡∏∏‡∏Ñ‡∏•‡∏¥‡∏Å‡∏õ‡∏±‡∏á‡πÅ‡∏ô‡πà‡∏ô‡∏≠‡∏ô ($percentText%) ‡∏Ñ‡∏∏‡∏ì‡∏£‡∏±‡∏Å‡∏©‡∏≤‡∏ü‡∏≠‡∏£‡πå‡∏°‡∏Å‡∏≤‡∏£‡∏ô‡∏±‡πà‡∏á‡πÅ‡∏ö‡∏ö‡∏ô‡∏µ‡πâ‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö",
